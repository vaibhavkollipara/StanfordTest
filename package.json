  },
  "dependencies": {
    "extract-text-webpack-plugin": "^1.0.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^4.1.2",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.9.1",
    "eslint": "^3.6.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.3.0",
    "file-loader": "^0.9.0",
    "mocha": "^3.1.0",
    "mocha-webpack": "^0.6.0",
    "null-loader": "^0.1.1",
    "react-addons-test-utils": "^15.3.2",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1",
    "webpack-node-externals": "^1.4.3"
  },
  "scripts": {
    "start": "webpack-dev-server --history-api-fallback --port 8080 --hot --inline",
    "prebuild": "npm run clean && npm run copy",
    "build": "cross-env NODE_ENV=production webpack -p --optimize-minimize",
    "clean": "rimraf src/build && mkdir src/build",
    "copy": "cp -r src/public/. src/build",
    "test": "cross-env NODE_ENV=test mocha-webpack --webpack-config webpack.config.test.js \"src/**/*test.js\"",
    "test:watch": "cross-env NODE_ENV=test mocha-webpack --webpack-config webpack.config.test.js --watch \"src/**/*test.js\""
  }
}
